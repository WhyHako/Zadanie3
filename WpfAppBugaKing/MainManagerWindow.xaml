<Window x:Class="WpfAppBugaKing.MainManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppBugaKing"
        mc:Ignorable="d"
        Title="BugaKing" Height="450" Width="800"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid AutoGenerateColumns="True" Height="300" VerticalAlignment="Top" x:Name="Manager_Grider">
        </DataGrid>
        <Button x:Name="Save" Content="Сохранить" Width="75" Height="50" HorizontalAlignment="Center" VerticalAlignment="Bottom" Click="Save_Click"/>
        <Image Source="img/image.png" Width="100" Height="100" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
        <Image Source="img/image.png" Width="100" Height="100" VerticalAlignment="Bottom" HorizontalAlignment="Right"/>
    </Grid>
</Window>
